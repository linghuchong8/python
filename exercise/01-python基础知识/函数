def hello():
    print("hello")
    
hello()

def sum(a,b):
    sum=a+b
    print(sum)
    
    
sum(5,10)


def calc(a, b):
    '''
    求a+b以及a-b的结果
    :param a:
    :param b:
    :return: 结果的元组
    '''
    sum = a + b
    sub = a-b
    return sum, sub

sum, sub = calc(10, 20)
print(sum)
print(sub)